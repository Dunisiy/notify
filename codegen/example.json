{
  "_broker": {
    "type": "broker",
    "name": "redis",
    "redis_client_config": {}
  },
  "_n": {
    "hello": "world"
  },
  "notify": {
    "inherit": ["_n"],
    "gen": {
      "location": "./notify_server.go"
    },
    "broker": "_broker",
    "id": "my-app",
    "pubsub": {
      "shards": 6,
      "shard_config": {
        "client_ttl": "10min",
        "client_invalidation_time": "5min",
        "client_buffer_size": "7min"
      },
      "pub_queue_config": {
        "buffer_size": 1024,
        "writers": 16,
        "readers_per_writer": 16
      },
      "clean_interval": "10min",
      "topic_buckets": 28
    },
    "server": {
      "name": "sockjs",
      "prefix": "/pubsub"
    },
    "server_goroutines": 16,
    "auth": {
      "name": "jwt",
      "secret": "some_secret_key"
    }
  },

  "api": {
    "some_data": "wefwef"
  }

}